@model IEnumerable<Medical_Records.Models.ApplicationUser>

<div class="container">
    <h2>My Profile</h2>
    @if (Model.Count() > 0)
    {
        @foreach (var userinfo in Model)
        {
            <p> @userinfo.UserName</p>
            <p> @userinfo.PhoneNumber</p>
            <p> @userinfo.CongenitalDesease</p>
            <p> @userinfo.CurrentMedication</p>
            <p> @userinfo.DrugAllergy</p>
        }

    }
    else
    {
        <p>No items created yet</p>
    }
</div>
